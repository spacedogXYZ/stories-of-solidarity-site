{
    "globals": {
        "Solidarity": true,
        "Backbone": true,
        "jQuery": true,
        "$": true,
        "_": true
    },
    "rules": {
        "indent": [0, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-console": 0,
        "comma-dangle": 0
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended"
}
